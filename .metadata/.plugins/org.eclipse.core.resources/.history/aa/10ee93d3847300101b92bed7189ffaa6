package com.lms;

import java.util.*;

public class Library {
	
	private List<Book> books;
    private List<User> users;
    
    public Library() {
    	books = new ArrayList<>();
    	users = new ArrayList<>();
    }

    public void addBook(Book book) {
    	books.add(book);
    }
    
    public void addUser(User user) {
    	users.add(user);
    }
    
}
